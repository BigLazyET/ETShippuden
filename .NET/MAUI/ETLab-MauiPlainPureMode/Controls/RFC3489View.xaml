<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ETLab_MauiPlainPureMode.Controls.RFC3489View">

    <StackLayout BindingContext="{Binding Source={RelativeSource TemplatedParent}}">
        <Label Text="NAT Type" />
        <Entry IsEnabled="False"
               Text="{Binding NATCheck3489Outcome.NATTYPE}"/>
        <Label Text="Local IPEndPoint" />
        <Entry IsEnabled="False"
               Text="{Binding NATCheck3489Outcome.LocalIPEndPoint}"/>
        <Label Text="Public IPEndPoint" />
        <Entry IsEnabled="False"
               Text="{Binding NATCheck3489Outcome.PublicIPEndPoint}"/>
    </StackLayout>
    
</ContentView>
